{
  "manifest_version": 2,

  "name": "Youtube English",
  "description": "Open Youtube English by current url",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Send to Youtube English"
  },

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "activeTab"
  ]

}
